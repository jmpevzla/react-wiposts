{
  "name": "rest-server",
  "version": "1.0.0",
  "description": "A mock backend api json-server",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node server.js --watch db.json --routes routes.json --port 4000 --static ./public",
    "watch": "nodemon server.js -- --watch db.json --routes routes.json --port 4000 --static ./public",
    "js": "node server.js --watch db.js --routes routes.json --port 4000 --static ./public --middlewares ./middlewares/middleware.js",
    "chalet-start": "chalet add 'npm start' -p 4000 && chalet start",
    "chalet-stop": "chalet stop",
    "open-browser": "google-chrome --proxy-pac-url=http://localhost:2000/proxy.pac"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6",
    "axios": "^0.26.0",
    "casual": "^1.6.2",
    "chalet": "^0.10.1",
    "chance": "^1.1.8",
    "dotenv": "^16.0.0",
    "json-server": "^0.17.0",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
